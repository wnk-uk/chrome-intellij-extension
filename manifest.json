{
    "manifest_version": 3,
    "name": "IntelliJ Element Opener",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "http://localhost:8080/*"
    ]
  }